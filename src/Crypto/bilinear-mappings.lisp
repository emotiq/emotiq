(defun gen-parms (trial)
  (let* ((pt (list (getf trial :x)
                   (getf trial :y)))
         (q  (getf trial :q))
         (p  (getf trial :p))
         (r  (getf trial :r))
         (curve (make-ecurve
                 :a  0
                 :b  1
                 :g  pt
                 :pmod p))
         (ptq (ecp-smul curve (* 12 r) pt)))
    (assert (not (eq ptq :infinity)))
    (let* ((s (random-between 2 q)))
      (list* :s    s
             :kpub (ecp-smul curve s ptq)
             :pgen ptq
             trial)) ))


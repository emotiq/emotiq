GCC=g++ -dynamiclib -I$(PREFIX)/include  -O2 -msse3 -mfpmath=sse -m64 -L. -L$(PREFIX)/lib

TARGET=$(PREFIX)/lib/libLispPBCIntf.dylib

all: $(TARGET)

$(TARGET): pbc_intf.cpp pbc_intf.h
	$(GCC) -dynamiclib -o libLispPBCIntf.dylib \
		pbc_intf.cpp \
                -lpbc -lgmp
	mv libLispPBCIntf.dylib $(TARGET)


